                            <StackPanel>
                                <Image Source="ms-appx:///RadialMenu/Assets/Redo.png" Height="22"/>
                                <TextBlock Text="Redo"/>
                            </StackPanel>
                        </Border>
                    </syncfusion:SfRadialMenuItem.Header>
                </syncfusion:SfRadialMenuItem >
                <syncfusion:SfRadialMenuItem x:ConnectionId='7'               IsEnabled="True" HorizontalAlignment="Center" VerticalAlignment="Center" >
                    <syncfusion:SfRadialMenuItem.Header>
                        <Border Background="Transparent" Padding="5">
                            <StackPanel>
                                <Image Source="ms-appx:///RadialMenu/Assets/Undo.png" Height="22"/>
                                <TextBlock Text="Undo"/>
                            </StackPanel>
                        </Border>
                    </syncfusion:SfRadialMenuItem.Header>
                </syncfusion:SfRadialMenuItem>
                <syncfusion:SfRadialMenuItem  >

                </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem  >

                    </syncfusion:SfRadialMenuItem>
                </syncfusion:SfRadialMenu>
        </Grid>
            
        </ContentControl>
    </View:SampleLayout>
</View:SampleLayout>

