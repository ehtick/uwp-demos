<View:SampleLayout
    x:Class="SampleBrowser.RadialMenu.RadialMenuView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SampleBrowser.RadialMenu" xmlns:View="using:Common"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"  xmlns:syncfusion="using:Syncfusion.UI.Xaml.Controls.Navigation"
    d:DesignHeight="300"
    d:DesignWidth="400">
    <UserControl.Resources>
        <syncfusion:EllipseSizeConverter x:Key="EllipseConverter"/>
        <Style x:Key="ImageStyle" TargetType="Image">
            <Setter Property="Width" Value="16"/>
            <Setter Property="Stretch" Value="Uniform"/>
        </Style>
        <Style  x:Key="TextBlockStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="12"/>
            <Setter Property="Margin" Value="3"/>
        </Style>
        <Style x:Key="NavigationButtonStyle" TargetType="Button">
            <Setter Property="Width" Value="95"/>
            <Setter Property="Height" Value="95"/>
            <Setter Property="FontFamily" Value="Segoe UI Symbol"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid Background="{x:Null}" 
                              RenderTransformOrigin="0.5 0.5">
                            <Grid.RenderTransform>
                                <CompositeTransform Rotation="-90"/>
                            </Grid.RenderTransform>
                            <Ellipse Fill="Gray" Stroke="Black" 
                                     StrokeThickness="1" 
                                     x:Name="BackgroundEllipse" />
                            <Ellipse Width="8" Height="8" Fill="White"
                                     Opacity="0.3" HorizontalAlignment="Left"
                                     Margin="28 25 0 0" VerticalAlignment="Top"/>
                            <Ellipse Width="8" Height="8" Fill="White"
                                     Opacity="0.3" VerticalAlignment="Top"
                                     Margin="0 25 0 0"/>
                            <Ellipse Width="8" Height="8" Fill="White"
                                     Opacity="0.3" HorizontalAlignment="Right"
                                     Margin="0 25 28 0"
                                     VerticalAlignment="Top"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

    </UserControl.Resources>
    <View:SampleLayout>
        <ContentControl HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" BorderThickness="2" BorderBrush="LightGray" Margin="5">
            <Grid.ChildrenTransitions>
                <TransitionCollection>
                    <PopupThemeTransition />
                </TransitionCollection>
            </Grid.ChildrenTransitions>
            <RichEditBox x:ConnectionId='1' IsSpellCheckEnabled="False"
                           Margin="10 10 0 10" FontSize="15"
                          Padding="10"
                           x:Name="text" BorderBrush="Transparent" BorderThickness="0"
                                                                     >


            </RichEditBox>
            <syncfusion:SfRadialMenu x:ConnectionId='2' HorizontalAlignment="Left"
                                   VerticalAlignment="Top" 
                                   EnableFreeRotation="False" 
                                     x:Name="selectionmenu" Padding="5">
                <syncfusion:SfRadialMenu.RenderTransform>
                    <CompositeTransform x:ConnectionId='8' x:Name="transform1"/>
                </syncfusion:SfRadialMenu.RenderTransform>
                <syncfusion:SfRadialMenu.Icon>
                    <Grid Background="Transparent">
                        <Image Source="ms-appx:///RadialMenu/Assets/text.png" Width="20" Stretch="Uniform"/>
                    </Grid>
                </syncfusion:SfRadialMenu.Icon>
                <syncfusion:SfRadialMenuItem x:ConnectionId='9'              HorizontalAlignment="Center" VerticalAlignment="Center">
                    <syncfusion:SfRadialMenuItem.Header>
                        <Border Background="Transparent" Padding="5">
                            <StackPanel>
                                <Image Source="ms-appx:///RadialMenu/Assets/Text-Bold.png" Height="22"/>
                                <TextBlock Text="Bold"/>
                            </StackPanel>
                        </Border>
                    </syncfusion:SfRadialMenuItem.Header>
                    <syncfusion:SfRadialMenuItem x:ConnectionId='69'              HorizontalAlignment="Center" VerticalAlignment="Center">
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image Source="ms-appx:///RadialMenu/Assets/Text-Bold.png"  Height="22"/>
                                    <TextBlock Text="Bold"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>

                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem x:ConnectionId='70'                HorizontalAlignment="Center" VerticalAlignment="Center">
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image Source="ms-appx:///RadialMenu/Assets/Text Italic.png"  Height="22"/>
                                    <TextBlock Text="Italic"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>

                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem x:ConnectionId='71'                   HorizontalAlignment="Center" VerticalAlignment="Center">
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image Source="ms-appx:///RadialMenu/Assets/subscript.png"  Height="22"/>
                                    <TextBlock Text="Subscript" FontSize="9" TextWrapping="Wrap"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>

                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem x:ConnectionId='72'                     HorizontalAlignment="Center" VerticalAlignment="Center">
                            <syncfusion:SfRadialMenuItem.Header>
                                <Border Background="Transparent" Padding="5">
                                    <StackPanel>
                                        <Image Source="ms-appx:///RadialMenu/Assets/superscript.png"  Height="22"/>
                                        <TextBlock Text="Superscript" FontSize="9" TextWrapping="Wrap"/>
                                    </StackPanel>
                                </Border>
                            </syncfusion:SfRadialMenuItem.Header>
                        </syncfusion:SfRadialMenuItem>
                        <syncfusion:SfRadialMenuItem x:ConnectionId='73'                   HorizontalAlignment="Center" VerticalAlignment="Center">
                            <syncfusion:SfRadialMenuItem.Header>
                                <Border Background="Transparent" Padding="5">
                                    <StackPanel>
                                        <Image Source="ms-appx:///RadialMenu/Assets/Underline.png"  Height="22"/>
                                        <TextBlock Text="Underline"/>
                                    </StackPanel>
                                </Border>
                            </syncfusion:SfRadialMenuItem.Header>

                        </syncfusion:SfRadialMenuItem>
                        <syncfusion:SfRadialMenuItem x:ConnectionId='74'                HorizontalAlignment="Center" VerticalAlignment="Center">
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image Source="ms-appx:///RadialMenu/Assets/strike.png"  Height="22"/>
                                        <TextBlock Text="Strike"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>

                    </syncfusion:SfRadialMenuItem>
                    
                        <syncfusion:SfRadialMenuItem x:ConnectionId='75' x:Name="bullet"                HorizontalAlignment="Center" VerticalAlignment="Center">
                            <syncfusion:SfRadialMenuItem.Header>
                                <Border Background="Transparent" Padding="5">
                                    <StackPanel>
                                        <Image Source="ms-appx:///RadialMenu/Assets/bullet.png" Height="16"/>
                                        <TextBlock Text="Bullets"/>
                                    </StackPanel>
                                </Border>
                            </syncfusion:SfRadialMenuItem.Header>
                        </syncfusion:SfRadialMenuItem>

                    </syncfusion:SfRadialMenuItem>

                <!--Cut Copy Paste-->
                <syncfusion:SfRadialMenuItem x:ConnectionId='10'              HorizontalAlignment="Center" VerticalAlignment="Center">
                    <syncfusion:SfRadialMenuItem.Header>
                        <Border Background="Transparent" Padding="5">
                            <StackPanel>
                                <Image Source="ms-appx:///RadialMenu/Assets/Copy.png"  Height="22"/>
                                <TextBlock Text="Copy"/>
                            </StackPanel>
                        </Border>
                    </syncfusion:SfRadialMenuItem.Header>
                    <syncfusion:SfRadialMenuItem />
                    <syncfusion:SfRadialMenuItem />
                    <syncfusion:SfRadialMenuItem />
                    <syncfusion:SfRadialMenuItem />
                    <syncfusion:SfRadialMenuItem x:ConnectionId='66'            >
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image Source="ms-appx:///RadialMenu/Assets/Cut.png"  Height="22"/>
                                    <TextBlock Text="Cut"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>
                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem x:ConnectionId='67'               >
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image Source="ms-appx:///RadialMenu/Assets/Copy.png"  Height="22"/>
                                    <TextBlock Text="Copy"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>
                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem x:ConnectionId='68'                >
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image Source="ms-appx:///RadialMenu/Assets/Paste.png"  Height="22"/>
                                    <TextBlock Text="Paste"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>
                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem  />
                    <syncfusion:SfRadialMenuItem  />
                    <syncfusion:SfRadialMenuItem  />
                </syncfusion:SfRadialMenuItem>

                <!--Undo Redo-->
                <syncfusion:SfRadialMenuItem x:ConnectionId='11' Name="Main_undo"              HorizontalAlignment="Center" VerticalAlignment="Center">
                    <syncfusion:SfRadialMenuItem.Header>
                        <Border Background="Transparent" Padding="5">
                            <StackPanel>
                                <Image x:ConnectionId='60' Name="undoImage1" Source="ms-appx:///RadialMenu/Assets/Undo.png"  Height="22"/>
                                <TextBlock x:ConnectionId='61' Name="Undotext1" Text="Undo"/>
                            </StackPanel>
                        </Border>
                    </syncfusion:SfRadialMenuItem.Header>
                    <syncfusion:SfRadialMenuItem  />
                    <syncfusion:SfRadialMenuItem  />
                    <syncfusion:SfRadialMenuItem  />
                    <syncfusion:SfRadialMenuItem x:ConnectionId='62' Name="Inner_Undo"              >
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image x:ConnectionId='64' Name="undoImage2" Source="ms-appx:///RadialMenu/Assets/Undo.png"  Height="22" />
                                    <TextBlock x:ConnectionId='65' Name="undotext2" Text="Undo"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>
                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem x:ConnectionId='63'              HorizontalAlignment="Center" VerticalAlignment="Center" >
                        <syncfusion:SfRadialMenuItem.Header>
                            <Border Background="Transparent" Padding="5">
                                <StackPanel>
                                    <Image Source="ms-appx:///RadialMenu/Assets/Redo.png"  Height="22"/>
                                    <TextBlock Text="Redo"/>
                                </StackPanel>
                            </Border>
                        </syncfusion:SfRadialMenuItem.Header>
                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialMenuItem  />
                    <syncfusion:SfRadialMenuItem  />
                </syncfusion:SfRadialMenuItem>

                <!--FontSize with SfRadial Slider-->
                <syncfusion:SfRadialMenuItem HorizontalAlignment="Center" VerticalAlignment="Center">
                    <syncfusion:SfRadialMenuItem.Header>
                        <Border Background="Transparent" Padding="5">
                            <StackPanel>
                                <TextBlock Padding="30,0,0,0" Text="{Binding ElementName=FontsizeSlider,Path=Value}" FontSize="9"/>
                                <Image  Source="ms-appx:///RadialMenu/Assets/a_inverte.png" Height="22"/>
                                <TextBlock Text="Font Size" />
                            </StackPanel>
                        </Border>
                    </syncfusion:SfRadialMenuItem.Header>

                    <syncfusion:SfRadialSlider x:ConnectionId='59' Margin="6" x:Name="FontsizeSlider" Value="15" SmallChange="5"                                           Minimum="0" Height="auto" Width="auto" Maximum="60" TickFrequency="5"/>
                </syncfusion:SfRadialMenuItem>

                <!--Color Pallete-->
                <syncfusion:SfRadialMenuItem x:ConnectionId='12' HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="colors">
                    <syncfusion:SfRadialMenuItem.Header>
                        <Border Background="Transparent" Padding="5">
                            <StackPanel>
                                <Image Source="ms-appx:///RadialMenu/Assets/Color.png" Height="22"/>
                                <TextBlock Text="Color"/>
                            </StackPanel>
                        </Border>
                    </syncfusion:SfRadialMenuItem.Header>
                    <syncfusion:SfRadialMenuItem x:ConnectionId='13' Name="paint" IsEnabled="False" >
                        <syncfusion:SfRadialMenuItem.Header>
                            <StackPanel>
                                <Image Source="ms-appx:///RadialMenu/Assets/Paint Bucket.png" HorizontalAlignment="Center" Height="22"/>
                                <Rectangle  Width="23" HorizontalAlignment="Center" Height="5" Fill="Red"/>
                                <TextBlock Text="Fill" HorizontalAlignment="Center" />
                            </StackPanel>
                        </syncfusion:SfRadialMenuItem.Header>
                    </syncfusion:SfRadialMenuItem>

                    <syncfusion:SfRadialMenuItem>
                        <syncfusion:SfRadialMenuItem.Header>
                            <StackPanel>
                                <Image  Source="ms-appx:///RadialMenu/Assets/a_inverte.png" HorizontalAlignment="Center" Height="22"/>
                                <Rectangle  Width="23" Height="5" HorizontalAlignment="Center"  Fill="Green"/>
                                <TextBlock HorizontalAlignment="Center" Text="Font" />
                            </StackPanel>
                        </syncfusion:SfRadialMenuItem.Header>
                    </syncfusion:SfRadialMenuItem>

                    <syncfusion:SfRadialMenuItem HorizontalAlignment="Center" VerticalAlignment="Center">
                        <syncfusion:SfRadialMenuItem.Header>
                            <StackPanel>
                                <Image Source="ms-appx:///RadialMenu/Assets/Highlighted.png" HorizontalAlignment="Center" Height="22"/>
                                <Rectangle  Width="23" Height="5" HorizontalAlignment="Center"  Fill="Blue"/>
                                <TextBlock Text="Highlight" HorizontalAlignment="Center"/>
                            </StackPanel>
                        </syncfusion:SfRadialMenuItem.Header>
                    </syncfusion:SfRadialMenuItem>
                    <syncfusion:SfRadialColorItem x:ConnectionId='14'               Color="#FFF6901E">
                        <syncfusion:SfRadialColorItem x:ConnectionId='51'               Color="#FFD16227"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='52'               Color="#FFEB7C23"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='53'               Color="#FFF6901E "/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='54'               Color="#FFFFC000"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='55'               Color="#FFFFD55F"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='56'               Color="#FFFFEB9C"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='57'               Color="#FFFFFF00"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='58'               Color="#FFAC4D25"/>
                    </syncfusion:SfRadialColorItem>

                    <syncfusion:SfRadialColorItem x:ConnectionId='15'                Color="#FFC00000">
                        <syncfusion:SfRadialColorItem x:ConnectionId='43'               Color="#FFFF00FF"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='44'               Color="#FF630331"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='45'               Color="#FF800000"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='46'               Color="#FFC00000"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='47'               Color="#FFFF0000"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='48'               Color="#FFF6A2A4"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='49'               Color="#FFFFC7CE"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='50'               Color="#FFE8158D"/>
                    </syncfusion:SfRadialColorItem>

                    <syncfusion:SfRadialColorItem x:ConnectionId='16'               Color="#FF008000">
                        <syncfusion:SfRadialColorItem x:ConnectionId='35'               Color="#FF00FF00"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='36'               Color="#FF1D421E"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='37'               Color="#FF1D5A2D"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='38'               Color="#FF1D5A2D"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='39'               Color="#FF008000"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='40'               Color="#FF00B050"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='41'               Color="#FF92D050"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='42'               Color="#FFC6EFCE"/>
                    </syncfusion:SfRadialColorItem>

                    <syncfusion:SfRadialColorItem x:ConnectionId='17'               Color="#FF002060">
                        <syncfusion:SfRadialColorItem x:ConnectionId='27'               Color="#FF00B0F0"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='28'               Color="#FF00FFFF"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='29'               Color="#FFC5F7FF"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='30'               Color="#FF7030A0"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='31'               Color="#FF800080"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='32'               Color="#FF002060"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='33'               Color="#FF0000FF"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='34'               Color="#FF0070C0"/>
                    </syncfusion:SfRadialColorItem>

                    <syncfusion:SfRadialColorItem x:ConnectionId='18'               Color="#FF000000">
                        <syncfusion:SfRadialColorItem x:ConnectionId='19'               Color="#FF646464"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='20'               Color="#FF747474"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='21'               Color="#FF969696"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='22'               Color="#FFB9B9B9"/>
                        <syncfusion:SfRadialColorItem x:ConnectionId='23'               Color="#FFD7D7D7"/>
