            <!--Initializes Diagram-->
            <syncfusion:SfDiagram x:Name="diagram" Grid.Column="1"  Background="White">
                <!--To Represent HorizontalRuler,VerticalRuler-->
                <syncfusion:SfDiagram.HorizontalRuler>
                    <controls:Ruler x:Name="_hruler" Orientation="Horizontal" Scale="{Binding ElementName=diagram,Path=ScrollInfo.CurrentZoom}"
                               Offset="{Binding ElementName=diagram,Path=ScrollInfo.HorizontalOffset}"
                               Thickness="25">
                    </controls:Ruler>
                </syncfusion:SfDiagram.HorizontalRuler>

                <syncfusion:SfDiagram.VerticalRuler>
                    <controls:Ruler x:Name="_vruler" Orientation="Vertical" Scale="{Binding ElementName=diagram,Path=ScrollInfo.CurrentZoom}"
                               Offset="{Binding ElementName=diagram,Path=ScrollInfo.VerticalOffset}"
                               Thickness="25">
                    </controls:Ruler>
                </syncfusion:SfDiagram.VerticalRuler>

                <!--SnapConstraints used to Control the Visibility of Gridlines and enable/disable Snapping.-->
                <syncfusion:SfDiagram.SnapSettings>
                    <syncfusion:SnapSettings SnapConstraints="All" SnapToObject="All"/>
                </syncfusion:SfDiagram.SnapSettings>

                <!--Initializes PageSettings-->
                <syncfusion:SfDiagram.PageSettings>
                    <syncfusion:PageSettings PageBorderBrush="Transparent" PageWidth="1000" PageHeight="600"
                                             PageBackground="White"/>
                </syncfusion:SfDiagram.PageSettings>

                <!--Initializes Node-->
                <syncfusion:SfDiagram.Nodes>
                    <syncfusion:NodeCollection>
                    </syncfusion:NodeCollection>
                </syncfusion:SfDiagram.Nodes>

                <!--Initializes Connector-->
                <syncfusion:SfDiagram.Connectors>
                    <syncfusion:ConnectorCollection>
                    </syncfusion:ConnectorCollection>
                </syncfusion:SfDiagram.Connectors>
            </syncfusion:SfDiagram>
