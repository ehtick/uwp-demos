    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Diagram"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:syncfusion="using:Syncfusion.UI.Xaml.Diagram"
    xmlns:controls="using:Syncfusion.UI.Xaml.Diagram.Controls"
    xmlns:stencil="using:Syncfusion.UI.Xaml.Diagram.Stencil"
    xmlns:common="using:Common"
    mc:Ignorable="d">
    <common:SampleLayout.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="ms-appx:///Template/DiagramBasicShapes.xaml"/>

            </ResourceDictionary.MergedDictionaries>
            <!--Style for Symbol-->
            <Style TargetType="stencil:Symbol">
                <Setter Property="Width" Value="50" />
                <Setter Property="Height" Value="50" />
                <Setter Property="Padding" Value="8" />
                <Setter Property="BorderThickness" Value="1" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="BorderBrush" Value="Transparent" />
                <Setter Property="Margin" Value="3"></Setter>
            </Style>

            <!--Style for Symbol Group-->
            <Style TargetType="stencil:SymbolGroup">
                <Setter Property="FontFamily" Value="Regular"/>
                <Setter Property="Background" Value="#ffffff"/>
                <Setter Property="Foreground" Value="#222222"/>
                <Setter Property="FontSize" Value="14"/>
                <Setter Property="HeaderTemplate">
                    <Setter.Value>
                        <DataTemplate>
                            <stencil:Header>
                                <stencil:Header.Template>
                                    <ControlTemplate TargetType="stencil:Header">
                                        <Grid>
                                            <Border x:Name="header" Background="#f5f5f5" BorderBrush="#dfdfdf" 
                                                BorderThickness="1">
                                                <ContentPresenter Margin="10" Content="{Binding}"/>
                                            </Border>
                                        </Grid>
                                    </ControlTemplate>
                                </stencil:Header.Template>
                            </stencil:Header>
                        </DataTemplate>
                    </Setter.Value>
                </Setter>
            </Style>


            <!--Style for Node-->
            <Style x:Key="shapeStyle" TargetType="Path">
                <Setter Property="Fill" Value="White"/>
                <Setter Property="Stroke" Value="#727272"/>
                <Setter Property="StrokeThickness" Value="4"/>
                <Setter Property="Stretch" Value="Fill"/>
            </Style>

            <Style x:Key="shapeStyle1" TargetType="Path">
                <Setter Property="Fill" Value="White"/>
                <Setter Property="Stroke" Value="#727272"/>
                <Setter Property="StrokeThickness" Value="2"/>
                <Setter Property="Stretch" Value="Fill"/>
            </Style>

            <!--Style for Port-->
            <Style x:Key="portShapeStyle" TargetType="Path">
                <Setter Property="Fill" Value="White"/>
                <Setter Property="Stroke" Value="Black"/>
                <Setter Property="Stretch" Value="Fill"/>
                <Setter Property="Height" Value="6"/>
                <Setter Property="Width" Value="6"/>
            </Style>
        </ResourceDictionary>
    </common:SampleLayout.Resources>

