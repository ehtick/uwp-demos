                                <Rectangle HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
          VerticalAlignment="Stretch"  Width="5"
              x:Name="PART_ReseizerThumb2"  StrokeDashCap="Flat" StrokeStartLineCap="Round" StrokeThickness="2" Margin="7" Stroke="Blue"  Fill="Blue" />
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <!--Style for Selector-->
            <Style x:Name="CustomSelector" TargetType="syncfusion:Selector">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate>
                            <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
