                                                      Fill="#FFFFFFFF" Stretch="Fill" HorizontalAlignment="Center" VerticalAlignment="Center" 
                                                      Width="15" Height="15"/>
                                            </Grid>
                                        </local:SelectorItem>
                                        <local:SelectorItem Command="{Binding Path=Graph.Commands.Delete}" 
                                                            Width="30" Height="30" Grid.Row="0" Grid.Column="2"
                                                            HorizontalAlignment="Right" VerticalAlignment="Top" ToolTipService.ToolTip="Delete">
                                            <Viewbox>
                                                <Grid>
                                                    <Grid Width="48" Height="48" Visibility="Visible">
                                                        <Ellipse Fill="#FF000000" Visibility="Visible" />
