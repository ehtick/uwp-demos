<Page x:Class="FloorPlannerDemo.ShapesStencil"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="using:FloorPlannerDemo"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:stencil="using:Syncfusion.UI.Xaml.Diagram.Stencil"
      mc:Ignorable="d">

    <Grid Background="#FF141414">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ContentControl Grid.Row="1" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" Content="{Binding}">
            <ContentControl.ContentTemplateSelector>
                <local:StencilDataTemplator></local:StencilDataTemplator>
            </ContentControl.ContentTemplateSelector>
        </ContentControl>
    </Grid>
</Page>