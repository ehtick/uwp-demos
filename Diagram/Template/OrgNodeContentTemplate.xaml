                       Grid.Column="1"
                       Margin="0,5,0,0"
                       VerticalAlignment="Center"
                       FontSize="13"
                       Foreground="White"
                       Text=":" />

                <TextBlock Grid.Row="1"
                       Grid.Column="2"
                       Margin="10,5,10,1"
                       FontFamily="Segoe UI"
                       FontSize="13"
                       Foreground="White"
                       Text="{Binding Path=Destination}" />
                <TextBlock Grid.Row="2"
                       Grid.Column="0"
                       Margin="20,5,5,10"
                       FontFamily="Segoe UI"
                       FontSize="13"
                       Foreground="White"
                       Text="Salary" />

                <TextBlock Grid.Row="2"
                       Grid.Column="1"
                       Margin="0,5,0,10"
                       VerticalAlignment="Center"
                       FontSize="13"
                       Foreground="White"
                       Text=":" />

                <TextBlock Grid.Row="2"
                       Grid.Column="2"
                       Margin="10,5,10,10"
                       FontFamily="Segoe UI"
                       FontSize="13"
                       Foreground="White"
                       Text="{Binding Path=Salary,
                                      Converter={StaticResource FormatConverter},
                                      ConverterParameter=$\{0:n0\}}" />
            </Grid>
        </ToolTip>
    </DataTemplate>
    
   

    <DataTemplate x:Key="contenteditor">
        <Grid Background="#FF353535">
            <Grid.RowDefinitions>
                <RowDefinition Height="25" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="1"
                       Margin="25,15,15,15"
                       FontFamily="Segoe UI"
                       FontSize="24"
                       FontWeight="Light"
                       Foreground="#FFE2E2E2"
                       Text="Employee Details" />

            <TextBlock Grid.Row="2"
                       Margin="25,20,5,5"
                       HorizontalAlignment="Left"
                       FontFamily="Segoe UI"
                       FontSize="17"
                       FontWeight="Normal"
                       Foreground="#FFE2E2E2"
                       Text="Name" />

            <text:SfTextBoxExt Grid.Row="3"
                               Width="200"
                               Margin="25,5,15,15"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               Foreground="#FF686868"
                               Text="{Binding Path=Name,
                                              Mode=TwoWay}" />

            <TextBlock Grid.Row="4"
                       Margin="25,7,5,5"
                       HorizontalAlignment="Left"
                       FontFamily="Segoe UI"
                       FontSize="17"
                       FontWeight="Normal"
                       Foreground="#FFE2E2E2"
                       Text="Designation" />

            <Grid Grid.Row="5"
                  Width="200"
                  Margin="25,5,15,15"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Center">
                <ComboBox Name="Designation"
                          FontFamily="Segoe UI Light"
                          Foreground="#FF686868"
                          ItemsSource="{Binding Designation}"
                          SelectedItem="{Binding Destination,
                                                 Mode=TwoWay}"
                          SelectedValuePath="Content" />
            </Grid>

            <TextBlock Grid.Row="6"
                       Margin="25,7,5,5"
                       HorizontalAlignment="Left"
                       FontFamily="Segoe UI"
                       FontSize="17"
                       FontWeight="Normal"
                       Foreground="#FFE2E2E2"
                       Text="e-mail" />

            <text:SfTextBoxExt Grid.Row="7"
                               Width="200"
                               Margin="25,5,15,15"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               Foreground="#FF686868"
                               Text="{Binding Path=EmailId,
                                              Mode=TwoWay}" />
            <TextBlock Grid.Row="8"
                       Margin="25,7,5,5"
                       HorizontalAlignment="Left"
                       FontFamily="Segoe UI"
                       FontSize="17"
                       FontWeight="Normal"
                       Foreground="#FFE2E2E2"
                       Text="Contact" />

            <text:SfNumericTextBox Grid.Row="9"
                                   Width="200"
                                   Margin="25,5,15,15"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   BlockCharactersOnTextInput="True"
                                   Foreground="#FF686868"
                                   FormatString="G"
                                   MaxLength="10"
                                   Value="{Binding Path=ContactNo,
                                                   Converter={StaticResource FormatConverter},
                                                   ConverterParameter=\{0\},
                                                   Mode=TwoWay}" />

            <TextBlock Grid.Row="10"
                       Margin="25,7,5,5"
                       HorizontalAlignment="Left"
                       FontFamily="Segoe UI Light"
                       FontSize="17"
                       Foreground="#FFE2E2E2"
                       Text="Salary" />

            <text:SfNumericTextBox Grid.Row="11"
                                   Width="200"
                                   Margin="25,5,15,15"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   BlockCharactersOnTextInput="True"
                                   Foreground="#FF686868"
                                   FormatString="G"
                                   Value="{Binding Path=Salary,
                                                   Mode=TwoWay}" />

            <TextBlock Grid.Row="12"
                       Margin="25,7,5,5"
                       HorizontalAlignment="Left"
                       FontFamily="Segoe UI Light"
                       FontSize="17"
                       Foreground="#FFE2E2E2"
                       Text="Rating" />

            <ComboBox Grid.Row="13"
                      Width="200"
                      Margin="25,5,15,15"
                      HorizontalAlignment="Left"
                      VerticalAlignment="Center"
                      FontFamily="Segoe UI"
                      FontSize="17"
                      Foreground="#FF686868"
                      ItemsSource="{Binding Rating}"
                      SelectedItem="{Binding RatingColor,
                                             Converter={StaticResource RatingToColorConverter},
                                             Mode=TwoWay}"
                      local:ComboBoxCommand.Command="{Binding RatingCommand}" />

            <TextBlock Grid.Row="14"
                       Margin="25,7,5,5"
                       HorizontalAlignment="Left"
                       FontFamily="Segoe UI Light"
                       FontSize="17"
                       Foreground="#FFE2E2E2"
                       Text="Gender"
                       Visibility="{Binding IsEdit,
                                            Converter={StaticResource BoolToVisibiltyConverter}}" />

            <ComboBox Grid.Row="15"
                      Width="200"
                      Margin="25,5,15,15"
                      HorizontalAlignment="Left"
                      VerticalAlignment="Center"
                      FontFamily="Segoe UI"
                      FontSize="17"
                      Foreground="#FF686868"
                      ItemsSource="{Binding Gender}"
                      SelectedItem="{Binding ImageUrl,
                                             Converter={StaticResource ItemtoImageConverter},
                                             Mode=TwoWay}"
                      Visibility="{Binding IsEdit,
                                           Converter={StaticResource BoolToVisibiltyConverter}}" />

        </Grid>
    </DataTemplate>

    <Style x:Key="styel" TargetType="Thumb">
        <Setter Property="Template" Value="{x:Null}" />
    </Style>

    <SolidColorBrush x:Key="PathStrokeThemeBrush" Color="#FFFFFFFF" />
    <x:Double x:Key="ConnectorThemeThickness">1</x:Double>
    <x:Double x:Key="ConnectorDecoratorThemeWidth">10</x:Double>
    <x:Double x:Key="ConnectorDecoratorThemeHeight">10</x:Double>
    <x:Double x:Key="PortThemeWidth">10</x:Double>
    <x:Double x:Key="PortThemeHeight">10</x:Double>
    
    
    <DataTemplate x:Key="OrganizationalemployeeContentTemplate">
        <Border BorderThickness="1" Background="{Binding Path=RatingColor}" CornerRadius="6" BorderBrush="LightGray">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="85" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0" >
                    <Border Grid.Column="0"
                          
                            VerticalAlignment="Stretch"
                            Background="Transparent"
                            BorderBrush="#FF5DC3B1"
                            CornerRadius="6"
                            Padding="5">
                        <Image HorizontalAlignment="Center"
                               Source="{Binding Path=ImageUrl}"
                               Stretch="Uniform" />
                    </Border>

                </Grid>
                <Grid Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="25" />
                        <RowDefinition Height="25" />
                    </Grid.RowDefinitions>

                    <TextBlock x:Name="Name"
                               Grid.Row="0"
                               Margin="15,10,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               FontFamily="Segoe UI"
                               FontSize="12"
                               FontWeight="Bold"
                               Foreground="#FF201F1A"
                               Text="{Binding Path=Name}"
                               TextAlignment="Left" />
                    <TextBlock x:Name="Destination"
                                    Grid.Row="1"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontFamily="Segoe UI"
                               FontSize="11"
                               FontWeight="SemiBold"
                               Foreground="#FF201F1A"
                               Text="{Binding Path=Destination}"
                               TextAlignment="Left" />
                </Grid>
            </Grid>

        </Border>
    </DataTemplate>
</ResourceDictionary>
