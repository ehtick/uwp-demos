    </Style>

    <Style x:Key="delete" TargetType="Button">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid Width="30" Height="30">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ColorAnimation Duration="0"
                                                        Storyboard.TargetName="rectangle"
                                                        Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                                                        To="#FF202021" />
                                        <ColorAnimation Duration="0"
                                                        Storyboard.TargetName="path"
                                                        Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                                                        To="White" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled" />
                                <VisualState x:Name="PointerOver">
                                    <Storyboard>
                                        <ColorAnimation Duration="0"
                                                        Storyboard.TargetName="rectangle"
                                                        Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                                                        To="#FFD3D3D6" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused" />
                                <VisualState x:Name="PointerFocused" />
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="rectangle"
                                   Width="30"
                                   Height="30"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Top"
                                   Fill="#FFF4F4F5"
                                   Stroke="Black" />
                        <Path x:Name="path"
                              Width="16"
                              Height="20"
                              Data="M33.978,27.679999L33.978,58.098995 41.374,58.098995 41.374,27.679999z M14.841001,27.679999L14.841001,58.098995 22.237,58.098995 22.237,27.679999z M4.0320003,22.430001L52.183002,22.430001 52.183002,63.995998 4.0320003,63.995998z M15.974001,0L40.194999,0 40.194999,7.7200003 56.166999,7.7200003 56.166999,15.995 0,15.995 0,7.7200003 15.974001,7.7200003z"
                              Fill="Black"
                              Stretch="Fill"
                              UseLayoutRounding="False" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <DataTemplate x:Key="ValueEditor">
        <Grid x:Name="editor">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>

            <TextBlock Grid.Row="0"
                       Margin="25,50,5,5"
                       FontFamily="Segoe UI"
                       FontSize="24"
                       FontWeight="Light"
                       Foreground="#FFE2E2E2"
                       Text="Properties" />

            <TextBlock Grid.Row="1"
                       Margin="25,10,5,5"
                       FontFamily="Segoe UI"
                       FontSize="17"
                       FontWeight="Light"
                       Foreground="#FFE2E2E2"
                       Text="Name" />

            <TextBox Grid.Row="2"
                     Grid.Column="0"
                     Width="250"
                     Margin="20,10,5,5"
                     FontFamily="Segoe UI"
                     FontSize="17"
                     FontWeight="Light"
                     Foreground="#FF686868"
                     Text="{Binding DispalyText,
                                    Mode=TwoWay}" />

            <ToggleSwitch x:Name="property"
                          Grid.Row="3"
                          Margin="25,10,5,5"
                          FontFamily="Segoe UI"
                          FontWeight="Light"
                          Foreground="#FFE2E2E2"
                          IsEnabled="{Binding IsNotDecision,
                                              Mode=TwoWay}"
                          IsOn="{Binding IsPropertiesEnabled,
                                         Mode=TwoWay}"
                          OffContent="Property Off"
                          OnContent="Property On" />
            <Border Grid.Row="4"
                    Grid.ColumnSpan="2"
                    Margin="25,10,5,5"
                    Visibility="{Binding Converter={StaticResource BoolToVisibilityConverter},
                                         Path=IsOn,
                                         ElementName=property}">
                <ItemsControl ItemsSource="{Binding Properties, Mode=TwoWay}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="45" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <TextBlock Grid.Row="0"
                                           Grid.Column="0"
                                           Margin="0,20,0,0"
                                           FontFamily="Segoe UI"
                                           FontSize="17"
                                           FontWeight="Light"
                                           Foreground="#FFE2E2E2"
                                           Text="Name" />

                                <TextBox Grid.Row="1"
                                         Grid.Column="0"
                                         Width="215"
                                         Margin="0,10,5,0"
                                         FontFamily="Segoe UI"
                                         FontSize="17"
                                         FontWeight="Light"
                                         Foreground="#FF686868"
                                         Text="{Binding Name,
                                                        Mode=TwoWay}" />

                                <Button Grid.Row="1"
                                        Grid.Column="1"
                                        Margin="0,10,5,0"
                                        Background="White"
                                        Command="{Binding DataContext.DeleteProperty,
                                                          ElementName=editor}"
                                        CommandParameter="{Binding}"
                                        Padding="1"
                                        Style="{StaticResource delete}" />

                                <RadioButton Grid.Row="2"
                                             Margin="0,20,0,0"
                                             Content="Numeric"
                                             FontFamily="Segoe UI"
                                             FontSize="12"
                                             FontWeight="Light"
                                             Foreground="#FFE2E2E2"
                                             IsChecked="{Binding Type,
                                                                 Converter={StaticResource NumericTypeToVisibilityConverter}}"
                                             local:RadioButtonCommand.Command="{Binding Checked}" />

                                <RadioButton x:Name="stringradio"
                                             Grid.Row="2"
                                             Margin="85,20,0,0"
                                             Content="String"
                                             FontFamily="Segoe UI"
                                             FontSize="12"
                                             FontWeight="Light"
                                             Foreground="#FFE2E2E2"
                                             IsChecked="{Binding Type,
                                                                 Converter={StaticResource StringTypeToBoolConverter}}"
                                             local:RadioButtonCommand.Command="{Binding Checked}" />

                                <TextBlock Grid.Row="3"
                                           Margin="0,20,0,0"
                                           FontFamily="Segoe UI"
                                           FontSize="17"
                                           FontWeight="Light"
                                           Foreground="#FFE2E2E2"
                                           Text="Units"
                                           Visibility="{Binding Type,
                                                                Converter={StaticResource TypeToVisibiltyConverter}}" />

                                <ComboBox Grid.Row="4"
                                          Grid.ColumnSpan="2"
                                          Width="250"
                                          Height="30"
                                          Margin="0,10,5,5"
                                          FontFamily="Segoe UI"
                                          FontSize="12"
                                          FontWeight="Light"
                                          Foreground="#FF686868"
                                          ItemsSource="{Binding ValueType}"
                                          SelectedItem="{Binding PropertyValue,
                                                                 Mode=TwoWay}"
                                          SelectedValuePath="Content"
                                          Visibility="{Binding Type,
                                                               Converter={StaticResource TypeToVisibiltyConverter}}" />

                                <!--  <Line Grid.Row="5" Grid.ColumnSpan="2" Stroke="White" Opacity="0.50" StrokeThickness="0.5"></Line>  -->
                                <Rectangle Grid.Row="5"
                                           Grid.ColumnSpan="2"
                                           Height="2"
                                           Margin="0,20,0,0"
                                           Opacity="0.50"
                                           Stroke="White"
                                           StrokeThickness="0.5" />

                            </Grid>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>

            </Border>
            <Button Grid.Row="5"
                    Margin="25,15,5,5"
                    Command="{Binding AddProperty}"
                    CommandParameter="{Binding}"
                    Content="AddProperty"
                    FontFamily="Segoe UI"
                    FontSize="17"
                    FontWeight="Light"
                    Foreground="#FFE2E2E2"
                    Visibility="{Binding Converter={StaticResource BoolToVisibilityConverter},
                                         Path=IsOn,
                                         ElementName=property}" />

            <Button Grid.Row="5"
                    Grid.Column="0"
                    Margin="180,15,5,5"
                    Command="{Binding Save}"
                    Content="Save"
                    FontFamily="Segoe UI"
                    FontSize="17"
                    FontWeight="Light"
                    Foreground="#FFE2E2E2"
                    Padding="32,3,32,3"
                    Visibility="{Binding Converter={StaticResource BoolToVisibilityConverter},
                                         Path=IsOn,
                                         ElementName=property}" />

        </Grid>
    </DataTemplate>
   

    <Style x:Key="nodestyle" TargetType="Path">
        <Setter Property="Fill" Value="DarkBlue" />
        <Setter Property="Stroke" Value="DarkGreen" />
        <Setter Property="StrokeThickness" Value="2" />
        <Setter Property="Stretch" Value="Fill" />
    </Style>

    <DataTemplate x:Key="EditTemplate">
        <TextBox   ManipulationMode="None" TextWrapping="Wrap" Foreground="{Binding Path=Content.Foreground,Mode=TwoWay}" AcceptsReturn="True" Text="{Binding Path=Content.Text, Mode=TwoWay}"/>
    </DataTemplate>


    <DataTemplate x:Key="ViewTemplate">
        <StackPanel Orientation="Vertical">
            <TextBlock Foreground="{Binding Path=Content.Foreground,Mode=TwoWay}" TextWrapping="Wrap" >
                    <Run Text="{Binding Path=Content.Text, Mode=TwoWay}"></Run>
            </TextBlock>
        </StackPanel>
    </DataTemplate>
    <Style x:Key="nodestyle1" TargetType="Path">
        <Setter Property="Fill" Value="Brown" />
        <Setter Property="Stroke" Value="YellowGreen" />
        <Setter Property="StrokeThickness" Value="2" />
        <Setter Property="Stretch" Value="Fill" />
    </Style>


    <Style x:Key="NormalLine" TargetType="Path">
        <Setter Property="Stroke" Value="Black" />
        <Setter Property="StrokeThickness" Value="1" />
    </Style>
    <Style x:Key="IntersectLine" TargetType="Path">
        <Setter Property="Stroke" Value="Black" />
        <Setter Property="Fill" Value="Black" />
        <Setter Property="StrokeThickness" Value="2" />
    </Style>
    <Style x:Key="SelectedLine" TargetType="Path">
        <Setter Property="Stroke" Value="Blue" />
        <Setter Property="Fill" Value="Blue" />
        <Setter Property="StrokeThickness" Value="2" />
    </Style>

    <DataTemplate x:Key="RNodeTemplate">
        <Border Background="{Binding Path=SelectedBrush}"
                BorderBrush="{Binding Path=SelectedBrush}"
                CornerRadius="12">
            <Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="CNodeTemplate">
        <Border Background="{Binding Path=SelectedBrush}"
                BorderBrush="{Binding Path=SelectedBrush}"
                CornerRadius="12">
            <Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </Border>
    </DataTemplate>
    <DataTemplate x:Key="LeafNodeTemplate">
        <Border 
                Background="Transparent"
              >
            <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Transparent">
            <Path StrokeThickness="2" Stroke="Black" Data="{Binding Path=path}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Fill="Transparent">

            </Path>
            <Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
            </Canvas>
        </Border>

    </DataTemplate>
    <DataTemplate x:Key="IntersectTemplate">
        <Border Background="{Binding Path=SelectedBrush}"
                BorderBrush="Black"
                BorderThickness="4"
                CornerRadius="12">
            <Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="SelectedTemplate">
        <Border Background="{Binding Path=SelectedBrush}"
                BorderBrush="Blue"
                BorderThickness="5"
                CornerRadius="12">
            <Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="vtemplate">
        <Border Padding="10">
            <TextBlock FontFamily="Segoe UI"
                       FontSize="15"
                       FontWeight="ExtraBold"
                       Text="{Binding Path=Content,
                                      Mode=TwoWay}" />
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="etempalte">
        <TextBox AcceptsReturn="True"
                 ManipulationMode="None"
                 Text="{Binding Path=Content,
                                Mode=TwoWay}"
                 util:FocusUtility.FocusOnLoad="True" />
    </DataTemplate>

</ResourceDictionary>
