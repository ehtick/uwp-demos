<Layout:SampleLayout x:Class="CellGridSamples.CellTypes"
                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:Layout="using:Common"
                     xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                     xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                     xmlns:syncfusion="using:Syncfusion.UI.Xaml.CellGrid"
                     mc:Ignorable="d">
    <Layout:SampleLayout.Resources>
        <DataTemplate x:Key="ButtonTemplate1">
            <Button x:Name="Button"
                    Width="138.5"
                    Height="38"
                    BorderBrush="#0078D7"
                    Click="Button_Click"
                    Content="Book Ticket"
                    Tag="3" />
        </DataTemplate>
        <DataTemplate x:Key="ButtonTemplate2">
            <Button x:Name="Button"
                    Width="138.5"
                    Height="38"
                    BorderBrush="#0078D7"
                    Click="Button_Click"
                    Content="Book Ticket"
                    Tag="5" />
        </DataTemplate>
        <DataTemplate x:Key="ButtonTemplate3">
            <Button x:Name="Button"
                    Width="138.5"
                    Height="38"
                    BorderBrush="#0078D7"
                    Click="Button_Click"
                    Content="Book Ticket"
                    Tag="7" />
        </DataTemplate>
    </Layout:SampleLayout.Resources>
    <Grid x:Name="grid" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <syncfusion:SfCellGrid x:Name="cellGrid">
            <syncfusion:SfCellGrid.Resources>
                <DataTemplate x:Key="ButtonTemplate">
                    <Button Width="138.5"
                            Height="38"
                            BorderBrush="#0078D7"
                            Click="Button_Click"
                            Content="Book Ticket" />
                </DataTemplate>
                <DataTemplate x:Key="Image1">
                    <Image Source="ms-appx:///Assets/Images/Avatar.jpeg" Stretch="Fill" />
                </DataTemplate>
                <DataTemplate x:Key="Image2">
                    <Image Source="ms-appx:///Assets/Images/Iceage3.jpeg" Stretch="Fill" />
                </DataTemplate>
                <DataTemplate x:Key="Image3">
                    <Image Source="ms-appx:///Assets/Images/shrek.jpeg" Stretch="Fill" />
                </DataTemplate>
            </syncfusion:SfCellGrid.Resources>
        </syncfusion:SfCellGrid>
    </Grid>
</Layout:SampleLayout>
